{
  "name": "tuinbouw",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "run:pwa": "npm run build && npm serve dist",
    "generate:translations": "npx tsx ./src/utils/sheet.ts",
    "lint": "npx eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "dev:emulate": "set VITE_EMULATION=true & npm run dev && npx firebase-tools emulators:start --only auth"
  },
  "dependencies": {
    "@google-cloud/local-auth": "^2.1.0",
    "@tomtom-international/web-sdk-maps": "^6.25.0",
    "@tomtom-international/web-sdk-services": "^6.25.0",
    "@vee-validate/yup": "^4.11.8",
    "@vue/apollo-composable": "^4.0.0-beta.11",
    "@vue/apollo-util": "^4.0.0-beta.6",
    "firebase": "^10.4.0",
    "googleapis": "^105.0.0",
    "logrocket": "^7.0.0",
    "lucide-vue-next": "^0.279.0",
    "primevue": "^3.37.0",
    "vee-validate": "^4.11.8",
    "vue": "^3.3.4",
    "vue-i18n": "^9.5.0",
    "vue-router": "^4.2.4",
    "yup": "^1.3.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.17.15",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-vue": "^9.18.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "typescript": "~5.1.6",
    "unocss": "^0.55.7",
    "vite": "^4.4.9",
    "vite-plugin-pwa": "^0.16.5",
    "vue-tsc": "^1.8.11"
  }
}